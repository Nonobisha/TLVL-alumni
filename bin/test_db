#!/usr/bin/env node

/**
 * Module dependencies.
 */

const app = require('../app');
const db = require('../db');
const debug = require('debug')('tlvl-alumni:server');
const http = require('http');

const User = require('../app_server/models/user');

User.deleteMany({}, (err) => console.log('Users collection removed'));

let john = new User({
  fullName: 'John Johnson',
  email: 'john@tlvl.ee',
  password: 'johnny',
  address: {
    country: 'EE',
    location: 'Tallinn',
  },
});
john.save();
console.log('John Johnson created');
console.log('Ctrl-C to quit');
